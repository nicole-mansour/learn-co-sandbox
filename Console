// declare the variable
var word

// assign a value to the variable
word = 'bird'

console.log(word)
// 'bird'

// assign another value to the variable
word = 'dog'

console.log(word)
// 'dog'

function convertToFahrenheit(celsius) {
if (typeof(celsius) === "undefined") {
var celsius = 0;
}
return (celsius * 9/5) + 32;
}